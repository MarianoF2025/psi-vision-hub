{
  "name": "centralwap-router",
  "version": "1.0.0",
  "description": "Enterprise WhatsApp Router - Reescritura Router PSI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:load": "k6 run tests/load/basic-flow.js",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier --write src/",
    "migrate": "node scripts/test-migration.js",
    "verify:webhooks": "node scripts/verificar-webhooks.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "typescript": "^5.3.2",
    "tsc-alias": "^1.8.8",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}


